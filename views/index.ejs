    <body>
		<!-- top bar -->
		<header class="navbar navbar-fixed-top" role="banner">
			<div class="container-fluid">
				<div class="navbar-header">
					<a href="dashboard.html" class="navbar-brand"><img src="assets/img/blank.gif" alt="Logo"></a>
				</div>			
				
			</div>
		</header>
		
		<!-- main content -->
		<div id="main_wrapper">
			<div class="page_content">
				<div class="container-fluid">
					<div class="row">
						<div class="col-md-12">
							<div class="panel panel-default">
								<div class="panel-body">
									<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>		
									<script type="text/javascript">
            						$(function(){				
                					$('#select_div').click(function(e){
                        			e.preventDefault();
                    
					         			var data = {};
					         			data.search = $('#gsc-i-id1').val();
					
					         			$.ajax({
						         			type: 'POST',
						         			data: JSON.stringify(data),
				                 			contentType: 'application/json',
                                 			url: 'http://localhost:3001/google-search',						
                                 			success: function(data) {
                                     			console.log('success');
                                     			$('#search-content').append(data);
                                     			console.log("data: ", JSON.stringify(data));                            
                                 			}
                             			});
 
                         			});	
	
                     			});
                 				</script>

                 				<h2>Dashboard</h2>
                 				<form class="gsc-search-box" action="/google-search" method='POST'>
        
           							<input type="text" name="search" title="search" id="gsc-i-id1" >     
           							<input id="select_div" type="button" value="Search" class="gsc-search-button" title="search">
         						</form>   								
								
								</div>
							</div>
						</div>
					</div>						

					<div class="row">
						<div class="col-md-12">
							<div class="panel panel-default">
								<div class="panel-body">
									<div id="chart"></div>
										<!-- Load d3.js and c3.js -->
										<script src="http://d3js.org/d3.v3.min.js"></script>
										<script src="javascripts/c3.min.js"></script>
										<script type="text/javascript">
    										var stock_price_open = ['Stock Price Open'];
    										var stock_price_high = ['Stock Price High'];
    										var stock_price_low = ['Stock Price Low'];
    										var stock_price_close = ['Stock Price Close'];
    										d3.csv("csv/NASDAQ_daily_prices.csv", function(tests)
											{
												data = tests.map(function(d)
												{
													stock_price_open.push(d.stock_price_open);
													stock_price_high.push(d.stock_price_high);
													stock_price_low.push(d.stock_price_low);
													stock_price_close.push(d.stock_price_close);
												})
    											var chart = c3.generate({
			    									bindto: '#chart',
			    									data: {
			      										columns: [
			            									stock_price_open,
			            									stock_price_high,
			            									stock_price_low,
			            									stock_price_close,
			        									],
			    									}
												});
											})	
    	
    									</script>
								</div>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="col-md-6">
							<div class="panel panel-default">
								<div class="panel-body">
									<div class="heading_b">Social Networks</div>
									<div class="row">
										<div class="col-md-12">
											<div id="flot_social" class="chart" style="width:100%">
											<div id="piechart"></div>
												<script type="text/javascript">
    												var stock_price_open = ['Stock Price Open'];
    												var stock_price_high = ['Stock Price High'];
    												var stock_price_low = ['Stock Price Low'];
    												var stock_price_close = ['Stock Price Close'];
    												var stock_price_adj_close = ['Stock Price Adj Close'];
    												d3.csv("csv/NASDAQ_daily_prices.csv", function(tests)
													{
														data = tests.map(function(d)
														{
															stock_price_open.push(d.stock_price_open);
															stock_price_high.push(d.stock_price_high);
															stock_price_low.push(d.stock_price_low);
															stock_price_close.push(d.stock_price_close);
															stock_price_adj_close.push(d.stock_price_adj_close);
														})
    													var chart = c3.generate({
			    											bindto: '#piechart',
			    											data: {
			      												columns: [
			            											stock_price_open,
			            											stock_price_high,
			            											stock_price_low,
			            											stock_price_close,
			            											stock_price_adj_close,
			        											],
			        											type : 'pie',
			        											onclick: function (d, i) { console.log("onclick", d, i); },
			        											onmouseover: function (d, i) { console.log("onmouseover", d, i); },
			        											onmouseout: function (d, i) { console.log("onmouseout", d, i); }
			    											},
    														legend: {
        														position: 'right'
    														}
														});
													})	
    	
    											</script>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="col-md-6">
							<div class="panel panel-default">
								<div class="panel-body">
									<div class="heading_b">Browsers</div>
									<div class="row">										
										<div class="col-md-12">
											<div id="flot_browsers" class="chart" style="width:100%">
											<div id="donutchart"></div>
												<script type="text/javascript">
    												var stock_price_open = ['Stock Price Open'];
    												var stock_price_high = ['Stock Price High'];
    												var stock_price_low = ['Stock Price Low'];
    												var stock_price_close = ['Stock Price Close'];
    												var stock_price_adj_close = ['Stock Price Adj Close'];
    												d3.csv("csv/NASDAQ_daily_prices.csv", function(tests)
													{
														data = tests.map(function(d)
														{
															stock_price_open.push(d.stock_price_open);
															stock_price_high.push(d.stock_price_high);
															stock_price_low.push(d.stock_price_low);
															stock_price_close.push(d.stock_price_close);
															stock_price_adj_close.push(d.stock_price_adj_close);
														})
    													var chart = c3.generate({
			    											bindto: '#donutchart',
			    											data: {
			      												columns: [
			            											stock_price_open,
			            											stock_price_high,
			            											stock_price_low,
			            											stock_price_close,
			            											stock_price_adj_close,
			        											],
			        											type : 'donut',
			        											onclick: function (d, i) { console.log("onclick", d, i); },
			        											onmouseover: function (d, i) { console.log("onmouseover", d, i); },
			        											onmouseout: function (d, i) { console.log("onmouseout", d, i); }
			    											},
    														legend: {
        														position: 'right'
    														},
			    											donut: {
			    												title: "Stock Price"
			    											}
														});
													})  	
    	
    											</script>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
