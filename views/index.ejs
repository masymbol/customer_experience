<!DOCTYPE html>
<html>
    <head>
		<meta charset="UTF-8">
		<title>Masymbol</title>
		<% include head %>
    </head>
    <body>
		<!-- top bar -->
		<% include header_content %>
		
		<!-- main content -->
		<div id="main_wrapper">
			<div class="page_content">
				<div class="container-fluid">
					<div id="wait" style="display: none;" ></div>
					<div class="row">
						<div class="col-md-12">
							<div class="panel panel-default">
								<div class="panel-body search-container">
         					<h2>Search </h2>
									<div class="form-group col-md-3">
										<form method="POST" action="/google_search">
											<div class="input-group search-container">
												<input class="form-control" name="search" id="search-text" type="text" placeholder = "Enter a search text">
												<span class="input-group-btn">
													<input id="btn_search_submit" value="Search" class="btn btn-primary" type="submit"></input>
												</span>
											</div>
										</form>
									</div>
								</div>
							</div>
						</div>
					</div>						

				</div>
			</div>		
		</div>

		<!-- side navigation -->
		<nav id="side_nav">
			<ul>
				<li>
					<a href="/preview"><span class="ion-speedometer"></span> <span class="nav_title">Dashboard</span></a>
				</li>		
				<li>
					<a href="/"><span class="glyphicon glyphicon-search"></span> <span class="nav_title">Search</span></a>
				</li>				
			</ul>
		</nav>

		<!-- bootstrap js plugins -->
		<script src="assets/bootstrap/js/bootstrap.min.js"></script>
		<!-- top dropdown navigation -->
		<script src="assets/js/tinynav.js"></script>		
	
		<!-- style switcher -->
		<script src="assets/js/tisa_style_switcher.js"></script>
		
    </body>
</html>