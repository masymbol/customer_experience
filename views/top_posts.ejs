<div class="col-md-6">
	<div class="panel panel-default">
		<div class="panel-body">
			<div class="heading_b topics_heading">Top Posts</div>
			<div class="row">
					<div class="col-md-12" style="height: 318px;">
						<div id="flot_social_top_posts" class="chart" style="width:100%">		
							<div id="posts">
								<span class="single-row-fixed">
										<a class="list-group-item">
											<span class="top-post-text"><b>Text</b></span>
											<span class="top-post-retweet-count" ><b style="float: right; margin-right: 10px;">Retweet-Count</b></span>
										</a>
								</span>
								<div class="list-group" id="posts-list-group">
									
							</div>
							</div>
							<script>			
								$.ajax({
							    url: "<%= disp_data.post_csv %>",
							    aync: false,
							    success: function (csvd) {
							    	var data = $.csv.toObjects(csvd);
							    	data.map(function(d)
							    	{
							    		if(d["text"]){
								    		var source_image;
								    		if(d["Source"] == "Twitter"){
								    			source_image = "<span class='ion-social-twitter social-icons new-social-twitter col-md-2'></span>";
								    		}else if(d["Source"] == "G+"){
								    			source_image = "<span class='ion-icon-social-google-plus social-icons new-social-google-plus col-md-2'></span>";
								    		} else{
								    			source_image = "<span class='ion-image social-icons'></span>"
								    		}

								    		$('#posts-list-group').append('<span class="single-row"><a href='+d["mix"]+' class="list-group-item" target="_blank">'+source_image+'<span class="top-post-text">'+d["text"]+'</span><span class="retweet_count "><strong>'+d["count_n_share"]+'</strong></span></span>');
											}
							    	});
							    },
							    dataType: "text"
								});

							</script>											
						</div>
					</div>
				</div>
		</div>
	</div>
</div>